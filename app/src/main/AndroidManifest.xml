<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wrinth.secondharvest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EventActivity"></activity>
        <activity android:name=".MemberActivity"></activity>
        <activity android:name=".NewEventActivity"></activity>
        <activity android:name=".EventListActivity"></activity>
        <activity android:name=".NewMemberActivity"></activity>
        <activity android:name=".MemberListActivity"></activity>
        <activity android:name=".NotificationActivity"></activity>
    </application>

</manifest>