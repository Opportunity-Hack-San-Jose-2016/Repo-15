<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wrinth.secondharvest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EventActivity"
            android:label="@string/event">

        </activity>
        <activity
            android:name=".MemberActivity"
            android:label="@string/member">

        </activity>
        <activity
            android:name=".NewEventActivity"
            android:label="@string/new_event">

        </activity>
        <activity
            android:name=".EventListActivity"
            android:label="@string/events_list">

        </activity>
        <activity
            android:name=".NewMemberActivity"
            android:label="@string/new_user">

        </activity>
        <activity
            android:name=".MemberListActivity"
            android:label="@string/users_list">

        </activity>
        <activity
            android:name=".NotificationActivity"
            android:label="@string/notification">

        </activity>
    </application>

</manifest>